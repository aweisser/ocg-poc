{
  "Enable": [
    "deadcode",
    "errcheck",
    "gas",
    "goconst",
    "gocyclo",
    "golint",
    "gotype",
    "gotypex",
    "ineffassign",
    "interfacer",
    "lll",
    "maligned",
    "megacheck",
    "misspell",
    "structcheck",
    "unconvert",
    "varcheck",
    "vet",
    "vetshadow"
  ],
  "LineLength": 100,
  "Exclude": [
    ".*_moq_test.go",
    "exported (.*) should have comment or be unexported",
    "comment on exported (.*) should be of the form (.*)"
  ]
}
